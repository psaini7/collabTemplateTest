  <calculation uuid="_OXr8oKG2EeakYckfWfj99g" name="Calculation_Rules"
      visibility="800">
    <text>//***************************product rules here*******************************************&#xD;
if (switchboard_height_toa==1800) kit_number =no_of_rack_slots_toa*12  +kit_posy +(kit_posx-1)*12;&#xD;
else kit_number =no_of_rack_slots_toa*14 + kit_posy +(kit_posx-1)*14;&#xD;
//***************************svg area - Start*******************************************&#xD;
if (world_show_busbars_toa &amp;&amp; position_category_kit==&quot;middle1&quot; ) display_busbars=&quot;inline&quot;; else display_busbars=&quot;none&quot;;&#xD;
if (world_show_busbars_toa ) display_busbars_npe=&quot;inline&quot;; else display_busbars_npe=&quot;none&quot;;&#xD;
if (world_show_kits_toa) {&#xD;
	scale_cursor=1;&#xD;
	display_kit=&quot;inline&quot;;&#xD;
	}&#xD;
	else {&#xD;
		scale_cursor=0;&#xD;
		display_kit=&quot;none&quot;;&#xD;
		}&#xD;
&#xD;
if (show_scale_highlighter &amp;&amp; world_show_kits_toa) scale_highlighter=1;&#xD;
else scale_highlighter=0;&#xD;
&#xD;
if (position_category_kit==&quot;bottom1&quot; || position_category_kit==&quot;bottom2&quot;) anchor_pos=1;&#xD;
else if (position_category_kit==&quot;top1&quot; || position_category_kit==&quot;top2&quot;) anchor_pos=2;&#xD;
else anchor_pos=0;&#xD;
&#xD;
if ((kit_posy+height)>=14) kit_slots13_14=1; &#xD;
else kit_slots13_14=0;&#xD;
&#xD;
//*************** PDC *****************&#xD;
/*When setting up the PDC coords remember that the 0,0 of configurator is not the same as PDC. The 0,0 of the configurator is in top right, in PDC it is lower left&#xD;
 * In the PDC additional pplinths are placed on top of the previous ones - thus raising the zero point of the cabinet&#xD;
 * In the configurator the plinths are placed on the bottom of previous ones - thus the zero point of the cabinet stays the same&#xD;
 * Therefor the plinth height must be calculated and added to any addtional position.&#xD;
 * The coords of the plinth is ALWAYS (0,0) in the PDC&#xD;
 */&#xD;
&#xD;
//Currently Build cannot take values from the SVG and used as input, therefore we must read them in the SVG and assign them manually here. &#xD;
height_column_profile = 57;&#xD;
width_column_profile = 50;&#xD;
height_main_busbar = 470;&#xD;
height_center_kit = 750;&#xD;
height_kit_top = 1;&#xD;
width_left_kit = column_width_toa - kit_width;&#xD;
&#xD;
//used for all left/right kits&#xD;
pdc_locationX_kit =translation_x_kitframes_PDC+ width_column_profile  + (kit_posx-1)*250; &#xD;
&#xD;
&#xD;
//used for n busbar, pe busbar and earthing within npe busbar kit&#xD;
//top&#xD;
pdc_locationX_top_n_busbar = pdc_locationX_kit;&#xD;
pdc_locationX_top_pe_busbar = pdc_locationX_kit;&#xD;
pdc_locationX_top_earthing = pdc_locationX_kit+80;&#xD;
//bottom&#xD;
pdc_locationX_bottom_n_busbar = pdc_locationX_kit;&#xD;
pdc_locationX_bottom_pe_busbar = pdc_locationX_kit;&#xD;
pdc_locationX_bottom_earthing = pdc_locationX_kit+40;&#xD;
&#xD;
pdc_locationY_kit = column_height_base_toa + height_column_profile + (kit_posy-1)*150; &#xD;
///used for kit when top&#xD;
//if(column_connection_toa == &quot;top&quot;){&#xD;
//	pdc_locationY_kit_top = column_height_base_toa + column_height_outside_toa - height_column_profile - kit_height - 600; &#xD;
//}&#xD;
//else{&#xD;
//	pdc_locationY_kit_top = column_height_base_toa + height_column_profile + height_center_kit + 600; &#xD;
//}&#xD;
//pdc_locationY_NPE_special_case_top = pdc_locationY_kit_top + 600-kit_height;&#xD;
&#xD;
//used for n busbar, pe busbar and earthing within npe busbar kit when top&#xD;
pdc_locationY_top_n_busbar = pdc_locationY_kit + kit_height - 200 - busbar_size_N/2;&#xD;
pdc_locationY_top_pe_busbar = pdc_locationY_kit + kit_height - 100 - busbar_size_PE/2;&#xD;
pdc_locationY_top_earthing = pdc_locationY_kit + kit_height - 200 - busbar_size_N/2-30;&#xD;
&#xD;
///used for kit when center and main busbar&#xD;
//if(column_connection_toa == &quot;top&quot;){&#xD;
//	pdc_locationY_kit_center = column_height_base_toa + column_height_outside_toa - height_column_profile - 600 - kit_height;&#xD;
//}&#xD;
//else{&#xD;
//	pdc_locationY_kit_center = column_height_base_toa + height_column_profile + 600;&#xD;
//}&#xD;
&#xD;
///used for kit when bottom&#xD;
//pdc_locationY_kit_bottom = column_height_base_toa +height_column_profile; &#xD;
//pdc_locationY_NPE_special_case_bottom = pdc_locationY_kit_bottom + kit_height;&#xD;
&#xD;
//used for n busbar, pe busbar and earthing within npe busbar kit when bottom&#xD;
pdc_locationY_bottom_n_busbar = pdc_locationY_kit + 200;&#xD;
pdc_locationY_bottom_pe_busbar = pdc_locationY_kit+ 100;&#xD;
pdc_locationY_bottom_earthing = pdc_locationY_kit + 5;&#xD;
&#xD;
//used for main busbar&#xD;
pdc_locationY_busbar_main = column_height_base_toa + column_height_outside_toa/2 - height_main_busbar/2;&#xD;
// element position&#xD;
kit_posx_string=kit_posx;&#xD;
kit_posy_string=kit_posy;&#xD;
element_position = column_number_toa+ &quot;.&quot; + column_rack_number + &quot;.&quot; + kit_posy_string;</text>
  </calculation>
