  <calculation uuid="_vEom4PUEEeejrv8UDV3dIw" name="Calculation_Rules"
      visibility="800">
    <text>//***************************svg area - Start*******************************************&#xD;
//width&#xD;
if (number_of_poles==&quot;3&quot;) width=105;&#xD;
//else if(RC_INST!=&quot;Unassigned&quot;) width=105;&#xD;
if (number_of_poles==&quot;4&quot;) width=140;&#xD;
//else if(RC_INST!=&quot;Unassigned&quot;) width=105;&#xD;
//else width=170;&#xD;
	&#xD;
//height&#xD;
//if ( RC_INST != &quot;Unassigned&quot;)height=223.5; 	&#xD;
//else if (POL==&quot;3&quot; &amp;&amp; RC_INST != &quot;Unassigned&quot;) height=217;&#xD;
//else if (RCINST==&quot;valPlugable&quot;) height=206;&#xD;
//else&#xD;
 height=150;&#xD;
&#xD;
height_rearbreaker1 = height;&#xD;
&#xD;
//if (POL==&quot;4&quot; &amp;&amp; RC_INST!=&quot;Unassigned&quot;) RL_Base_width=width-5.2-30;&#xD;
//else &#xD;
RL_Base_width=width-5.2;&#xD;
&#xD;
RL_Drive_move_x=width-2.6-27.4-45-95;&#xD;
RL_move_x=width-2.6-RL_Base_width-32.59998;//134.8&#xD;
PathP3_PathP4_move_x=width-2.6-134.8-32.59998;&#xD;
&#xD;
&#xD;
Drive_move_x=width-2.6-27.4-45/2-(79+76/2) ;&#xD;
&#xD;
&#xD;
&#xD;
if (device_command==&quot;motorized&quot;)&#xD;
{&#xD;
	visiblity_RLDrive=&quot;hidden&quot;;&#xD;
	visiblity_motorDrive=&quot;visible&quot;;&#xD;
		visiblity_pathP4=&quot;hidden&quot;;&#xD;
	visiblity_pathp3=&quot;hidden&quot;;&#xD;
}&#xD;
else&#xD;
{&#xD;
	if (number_of_poles==&quot;4&quot;)&#xD;
{&#xD;
	visiblity_pathP4=&quot;visible&quot;;&#xD;
	visiblity_pathp3=&quot;hidden&quot;;&#xD;
}&#xD;
else&#xD;
{&#xD;
	visiblity_pathP4=&quot;hidden&quot;;&#xD;
	visiblity_pathp3=&quot;visible&quot;;&#xD;
}	&#xD;
	visiblity_motorDrive=&quot;hidden&quot;;&#xD;
	visiblity_RLDrive=&quot;visible&quot;;&#xD;
}&#xD;
visiblity_Rotary_Drive_Axle=&quot;hidden&quot;;&#xD;
visiblity_Rotary_Drive=&quot;hidden&quot;;&#xD;
visiblity_RC_Sel=&quot;hidden&quot;;&#xD;
&#xD;
if(device_command == &quot;motorized&quot;) {&#xD;
	move_x_flanges = Drive_move_x + 79 - 6;&#xD;
	move_y_flanges = 36 - 16;&#xD;
}&#xD;
else {&#xD;
	move_x_flanges = RL_move_x + 32.6 - 8;&#xD;
	move_y_flanges = 62.5 - 18.5;&#xD;
}</text>
  </calculation>
