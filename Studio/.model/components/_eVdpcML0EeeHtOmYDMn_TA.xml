  <calculation uuid="_iull0ML0EeeHtOmYDMn_TA" name="Calculation_Rules"
      visibility="800">
    <text>	if (display_device==&quot;none&quot;) {&#xD;
		display_compensator_XLH=&quot;none&quot;;&#xD;
		display_compensator=&quot;none&quot;;&#xD;
	}&#xD;
	else&#xD;
	{&#xD;
		if (column_type_toa==&quot;xline_hor&quot;) {&#xD;
			device_rotation_rbbs =90;&#xD;
			device_trans_x_rbbs = module_height+5;&#xD;
			device_trans_y_rbbs =0;&#xD;
			move_visual_cursor_rbbs=visual_height;&#xD;
			display_compensator_XLH=&quot;inline&quot;;&#xD;
			display_compensator=&quot;none&quot;;&#xD;
			}&#xD;
			else if (column_type_toa==&quot;switch_disconnector_fuse&quot;)		{&#xD;
			rotation_switch_fuse_compensator=90;&#xD;
			device_trans_x_rbbs = visual_width;&#xD;
			device_trans_y_rbbs =0;&#xD;
			move_visual_cursor_rbbs=0;&#xD;
			display_compensator_XLH=&quot;inline&quot;;&#xD;
			display_compensator=&quot;none&quot;;&#xD;
			}&#xD;
		else&#xD;
			{&#xD;
				device_rotation_rbbs =0;&#xD;
			device_trans_x_rbbs = 0;&#xD;
			device_trans_y_rbbs =0;&#xD;
			move_visual_cursor_rbbs=0;&#xD;
			display_compensator_XLH=&quot;none&quot;;&#xD;
			display_compensator=&quot;inline&quot;;&#xD;
			}	&#xD;
	}&#xD;
	&#xD;
elementText_transalation = elementText_transalation - 500;&#xD;
elementText_transalation_N = elementText_transalation_N - 350;&#xD;
&#xD;
//pdc_design_info_position//&#xD;
if(column_type_toa == &quot;switch_disconnector_fuse&quot; &amp;&amp; column_width_toa == 1250)&#xD;
{&#xD;
if(kit_width == 750)	&#xD;
kit_pdc_center_right_X = kit_pdc_center_X + column_width_toa - (kit_width+500);&#xD;
else if(kit_width == 500)&#xD;
kit_pdc_center_right_X = kit_pdc_center_X + column_width_toa - (kit_width+750);&#xD;
}&#xD;
else&#xD;
kit_pdc_center_right_X = kit_pdc_center_X + column_width_toa - kit_width;&#xD;
&#xD;
width_devices_left = (device_pos_x-1)*50;&#xD;
&#xD;
//PDC LocationX and Y&#xD;
if(column_type_toa==&quot;fuse_switch_disconnector&quot;){&#xD;
pdc_locationX_device = kit_pdc_center_X + 25 + width_devices_left;&#xD;
}&#xD;
else{pdc_locationX_device = kit_pdc_center_X + 50 + width_devices_left;&#xD;
}&#xD;
pdc_locationX_right_device =  kit_pdc_center_right_X + (kit_width-400)/2 + width_devices_left;&#xD;
pdc_locationY_device = kit_pdc_center_Y + (kit_height - module_height)/2;&#xD;
&#xD;
//pdc_design_info_position//&#xD;
</text>
  </calculation>
