  <calculation uuid="_btKgIPQ5EeeZWekAE_d58Q" name="Calculation_Rules"
      visibility="800">
    <text>//***************************svg area - Start*******************************************&#xD;
//width&#xD;
if (number_of_poles==&quot;3&quot;) width=76.2;//76.2;&#xD;
//else if(RC_INST != &quot;Unassigned&quot;) 	width=131.2;	&#xD;
else width=101.2;//101.2;&#xD;
	&#xD;
//height	&#xD;
//if (POL==&quot;4&quot; &amp;&amp; RC_INST != &quot;Unassigned&quot;) height=187;&#xD;
//else&#xD;
 height=130;&#xD;
&#xD;
height_rearbreaker1 = height;&#xD;
&#xD;
//width Rock Lever&#xD;
//if (POL==&quot;4&quot; &amp;&amp; RC_INST != &quot;Unassigned&quot;) width_RL_base=width-5.2-30; //(30 is width of RC_SEL )&#xD;
//else&#xD;
 width_RL_base=width-5.2;&#xD;
&#xD;
RL_drive_movex=-70.59998 + width-2.6-13-45;&#xD;
&#xD;
if (number_of_poles ==&quot;4&quot;)&#xD;
{&#xD;
	RL_movex=width-2.6-width_RL_base-32.59999;&#xD;
	path_movex=width-2.6-width_RL_base/2-32.59999-width_RL_base/2;&#xD;
}&#xD;
else&#xD;
{&#xD;
	RL_movex=width/2-33-width_RL_base/2;&#xD;
	path_movex=width/2-width_RL_base/2-24-width_RL_base/2;&#xD;
	//path_movex=width_rb-width_RL_base/2-33-width_RL_base/2;&#xD;
}&#xD;
//movex_centre=70.5998+45/2-55.0999-76/2;&#xD;
movex_centre=width -2.6-13-45/2 - 56-76/2;&#xD;
//RL_Move_y=49;&#xD;
if (device_command==&quot;motorized&quot;)&#xD;
{&#xD;
	visiblity_RLDrive=&quot;hidden&quot;;&#xD;
	visiblity_motorDrive=&quot;visible&quot;;&#xD;
		visiblity_pathP4=&quot;hidden&quot;;&#xD;
	visiblity_pathp3=&quot;hidden&quot;;&#xD;
}&#xD;
else&#xD;
{&#xD;
	if (number_of_poles==&quot;4&quot;)&#xD;
{&#xD;
	visiblity_pathP4=&quot;visible&quot;;&#xD;
	visiblity_pathp3=&quot;hidden&quot;;&#xD;
}&#xD;
else&#xD;
{&#xD;
	visiblity_pathP4=&quot;hidden&quot;;&#xD;
	visiblity_pathp3=&quot;visible&quot;;&#xD;
}&#xD;
	visiblity_motorDrive=&quot;hidden&quot;;&#xD;
	visiblity_RLDrive=&quot;visible&quot;;&#xD;
}&#xD;
visiblity_Rotary_Drive_Axle=&quot;hidden&quot;;&#xD;
visiblity_Rotary_Drive=&quot;hidden&quot;;&#xD;
visiblity_RC_Sel=&quot;hidden&quot;;&#xD;
&#xD;
if(device_command == &quot;motorized&quot;) {&#xD;
	move_x_flanges = movex_centre + 55.1 - 7.5;&#xD;
	move_y_flanges = 17 - 17.3;&#xD;
}&#xD;
else {&#xD;
	move_x_flanges = -5.9;&#xD;
	move_y_flanges = 49 - 18.5;&#xD;
}&#xD;
&#xD;
//***************************svg End - Start*******************************************</text>
