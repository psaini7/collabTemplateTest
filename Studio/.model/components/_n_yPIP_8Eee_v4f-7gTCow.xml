  <calculation uuid="_r58-oP_8Eee_v4f-7gTCow" name="Calculation_Rules"
      visibility="800">
    <text>//***************************product rules here*******************************************&#xD;
if (switchboard_height_toa==1800) kit_number =no_of_rack_slots_toa*12 + kit_posy +(kit_posx-1)*12;&#xD;
else kit_number =no_of_rack_slots_toa*14 + kit_posy +(kit_posx-1)*14;&#xD;
&#xD;
//***************************svg area - Start*******************************************&#xD;
if (world_show_doors_toa) {&#xD;
	display_doors_twinline=&quot;inline&quot;; &#xD;
	scale_cursor_door=1;&#xD;
	}&#xD;
	else {&#xD;
		display_doors_twinline=&quot;none&quot;;&#xD;
		scale_cursor_door=0;&#xD;
	}&#xD;
if (world_show_busbars_toa) display_busbars_twinline=&quot;inline&quot;; else display_busbars_twinline=&quot;none&quot;;&#xD;
&#xD;
if (world_show_kits_toa) {&#xD;
	scale_cursor=1;&#xD;
	display_kit_twinline=&quot;inline&quot;;&#xD;
	}&#xD;
	else {&#xD;
		scale_cursor=0;&#xD;
		display_kit_twinline=&quot;none&quot;;&#xD;
		}&#xD;
if (world_show_front_covers_toa) display_front_covers_twinline=&quot;inline&quot;; else display_front_covers_twinline=&quot;none&quot;;&#xD;
&#xD;
//***************************svg area - End*******************************************&#xD;
&#xD;
&#xD;
//PDC&#xD;
//Currently Build cannot take values from the SVG and used as input, therefore we must read them in the SVG and assign them manually here. &#xD;
height_column_profile = 57;&#xD;
width_column_profile = 50;&#xD;
height_main_busbar = 470;&#xD;
height_center_kit = 750;&#xD;
height_kit_top = 1;&#xD;
width_left_kit = column_width_toa - kit_width;&#xD;
 pdc_locationX_kit =translation_x_kitframes_PDC+ width_column_profile  + (kit_posx-1)*250;&#xD;
 pdc_locationY_kit = column_height_base_toa + height_column_profile + (kit_posy-1)*150;  &#xD;
 pdc_locationY_kit_twinline=pdc_locationY_kit-50;&#xD;
 &#xD;
 // element position&#xD;
temp_kit_posx =kit_posx;&#xD;
&#xD;
if (column_rack_number==1)temp_kit_posx = kit_posx;&#xD;
 else if (column_rack_number==2){&#xD;
	if (column_width_toa ==1000) temp_kit_posx = kit_posx +2;&#xD;
	else if (column_width_toa ==1250) temp_kit_posx = kit_posx +3;&#xD;
}&#xD;
&#xD;
kit_posx_string = temp_kit_posx;&#xD;
 kit_posy_string=kit_posy;&#xD;
&#xD;
element_position = column_number_toa+ &quot;.&quot; + kit_posx_string + &quot;.&quot; + kit_posy_string;&#xD;
/*&#xD;
if (column_connection_toa == &quot;bottom&quot; &amp;&amp; (kit_posy+height)>=11) offset_n185_legal = 1;&#xD;
else if (column_connection_toa == &quot;top&quot; &amp;&amp; (kit_posy+height)&lt;=5) offset_n185_legal = 1;&#xD;
else offset_n185_legal = 0; */&#xD;
if (column_connection_toa == &quot;bottom&quot; &amp;&amp; switchboard_height_toa==1800 &amp;&amp; (kit_posy+height)>=11) offset_n185_legal = 1;&#xD;
else if (column_connection_toa == &quot;bottom&quot; &amp;&amp; switchboard_height_toa==2100 &amp;&amp; (kit_posy+height)>=13) offset_n185_legal = 1;&#xD;
else if (column_connection_toa == &quot;top&quot; &amp;&amp; (kit_posy+height)&lt;=5) offset_n185_legal = 1;&#xD;
else offset_n185_legal = 0;</text>
  </calculation>
